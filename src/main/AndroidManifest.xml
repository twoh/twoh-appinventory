<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.web.twoh.appinventory"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="id.web.twoh.appinventory.activity.CreateData"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="id.web.twoh.appinventory.activity.Menu"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="id.web.twoh.appinventory.activity.ViewData"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="id.web.twoh.appinventory.activity.EditData"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="id.web.twoh.appinventory.activity.ViewSingleData"
            android:label="@string/title_activity_main" >
        </activity>
    </application>

</manifest>